sudo apt-get update;
sudo apt-get install nodejs;
sudo apt-get install npm;
echo "Announcing version of Node";
nodejs -v;
echo "Announcing version of npm";
npm -v;
echo "Attempting to install noodle-deploy";
npm install noodle-deploy;
echo "Now Trying to use noodle-deploy";
sudo noodle-deploy -s $AES_PASS -p HuelPicker -t "build/app.js";
